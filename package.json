  "scripts": {
    "postinstall": "npm run bundle:env && npm run bundle:production",
    "start": "npm run start:production",
    "stop": "npm run tomcat:stop && npm run solr:stop && npm run server:stop",
    "start:production": "npm run solr:start && npm run tomcat:start && npm run server:start",
    "start:develop": "npm run solr:start && npm run tomcat:start && npm run server:dev & npm run bundle:watch",
    "solr:start": "npm run solr:stop && ./lib/solr/bin/solr start",
    "solr:stop": "./lib/solr/bin/solr stop -all",
    "tomcat:start": "npm run tomcat:stop && ./lib/tomcat/bin/startup.sh",
    "tomcat:stop": "./lib/tomcat/bin/shutdown.sh",
    "bundle:env": "bin/env",
    "bundle:production": "./node_modules/.bin/webpack --progress --colors -p",
    "bundle:dev": "./node_modules/.bin/webpack -d",
    "bundle:watch": "./node_modules/.bin/webpack --progress --colors --watch -d",
    "server:start": "./node_modules/.bin/forever start -m 1 -l ./melodicsimilarity-react.log -a bin/www",
    "server:stop": "./node_modules/.bin/forever stop bin/www",
    "server:dev": "./node_modules/.bin/nodemon bin/www",
    "test": "jest --verbose"
  },
  "jest": {
    "rootDir": "./",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/client/test/styleMock.js",
      "^.+\\.(png|woff|woff2|ttf|eot|svg|mp3)$": "<rootDir>/client/test/fileMock.js"
    }
  }
